{
    "name": "saga/dic-module",
    "description": "This module provides the Dependency Injection Container integration in Drupal 7.",
    "type": "drupal-module",
    "require": {
        "symfony/dependency-injection": "~2.4",
        "symfony/config": "~2.4",
        "symfony/yaml": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*"
    },
    "extra": {
        "installer-name": "dic",
        "branch-alias": {
            "dev-master": "7.1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\dic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\dic\\Tests\\": "tests/"
        }
    }
}